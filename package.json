{
    "name": "chaiscript-syntax",
    "displayName": "ChaiScript Syntax",
    "description": "Syntax support for ChaiScript(https://chaiscript.com/)",
    "version": "0.0.6",
    "publisher": "codehz",
    "engines": {
        "vscode": "^1.25.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/codehz/vscode-chaiscript-syntax"
    },
    "contributes": {
        "languages": [{
            "id": "chaiscript",
            "aliases": ["ChaiScript", "chaiscript"],
            "extensions": [".chai"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "chaiscript",
            "scopeName": "source.chai",
            "path": "./syntaxes/chaiscript.tmLanguage.json"
        }]
    }
}